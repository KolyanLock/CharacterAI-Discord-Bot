{
  "discord_bot_token": "${DISCORD_BOT_TOKEN}",
  "char_ai_user_token": "${CHAR_AI_USER_TOKEN}",
  "manage_character_id": "${MANAGE_CHARACTER_ID}",

  "discord_bot_role": "Manage Character",
  "discord_bot_prefixes": [ ".an ", "!!" ],
  "discord_private_category_name": "CAI PRIVATE CHANNELS",

  "default_audience_mode": 3,
  "default_no_permission_file": "nopower.gif",

  "private_chat_role_required": false,
  "use_character_avatar": true,
  "use_character_name": true,
  "description_in_playing_status": true,
  "allow_dm": true,

  "dead_chat_message": "${DEAD_CHAT_MESSAGE}",
  "reply_chance": "${REPLY_CHANCE}",
  "rate_limit": "${RATE_LIMIT}",
  "message_timeout_mins": "${MESSAGE_TIMEOUT_MINS}",
  "messages_buffer_length": "${MESSAGES_BUFFER_LENGTH}",
  "inactive_message_count": "${INACTIVE_MESSAGE_COUNT}",
  "enable_swipe_buttons": false,
  "buttons_remove_delay": 60,

  "auto_setup": true,
  "auto_char_id": "${AUTO_CHAR_ID}"
}